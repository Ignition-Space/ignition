{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/ignition-server/src",
  "monorepo": true,
  "root": "apps/ignition-server",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/ignition-server/tsconfig.app.json"
  },
  "projects": {
    "ignition-server": {
      "type": "application",
      "root": "apps/ignitionServer",
      "entryFile": "main",
      "sourceRoot": "apps/ignitionServer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ignitionServer/tsconfig.app.json"
      }
    },
    "materials-server": {
      "type": "application",
      "root": "apps/materialsServer",
      "entryFile": "main",
      "sourceRoot": "apps/materialsServer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/materialsServer/tsconfig.app.json"
      }
    },
    "user-server": {
      "type": "application",
      "root": "apps/userServer",
      "entryFile": "main",
      "sourceRoot": "apps/userServer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/userServer/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}