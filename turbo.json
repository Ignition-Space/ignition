{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:user": {
      "cache": false,
      "persistent": true
    },
    "dev:ig": {
      "cache": false,
      "persistent": true
    },
    "build:user": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "deploy:user": {
      "dependsOn": [
        "^deploy"
      ],
      "outputs": []
    }
  }
}