  "scripts": {
    "dev": "turbo run dev",
    "dev:user": "turbo run dev:user",
    "dev:ig": "turbo run dev:ig",
    "dev-server:ig": "cross-env RUNNING_ENV=dev nest start --watch",
    "dev-server:user": "cross-env RUNNING_ENV=dev nest start --watch user-server",
    "dev-server:materials": "cross-env RUNNING_ENV=dev nest start --watch materials-server",
    "dev-client:user": "pnpm --filter user-center run dev:user",
    "dev-server:devops": "cross-env RUNNING_ENV=dev nest start --watch devops-server",
    "prebuild": "rimraf dist",
    "clean": "rimraf dist && rimraf node_modules && rimraf **/*/node_modules",
    "build": "turbo run build",
    "build:user": "turbo run build:user",
    "build-server:user": "nest build user-server",
    "start-server:user": "node ./dist/apps/userServer/src/main.js",
    "deploy:user": "turbo run deploy:user",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\""
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.1.1",
