    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "esbuild": "^0.18.20",
    "history": "^5.3.0",
    "invariant": "^2.2.4",
